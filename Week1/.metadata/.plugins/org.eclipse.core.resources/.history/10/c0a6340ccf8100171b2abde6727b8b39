package com.ex.day2;

public class StringBufferAndBuilder {
	
	public static void main(String[] args) {
		String str = "hello";
		StringBuffer buff = new StringBuffer("hello");
		StringBuilder build = new StringBuilder("hello");
		
		
		str.concat("world");
		buff.append("world");
		build.append("world");
		int d = 3;
		d =+ 2;
		System.out.println(d);
	}

}
