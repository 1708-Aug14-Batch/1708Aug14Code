package genres;

public abstract class Genre implements Readable{
	
	public void open(){
		System.out.println("You opened the book.");
		
	}
	
	public void close(){
		System.out.println("You closed the book.");
	}
	
	public int read(){
		System.out.println("You want to read, but don't have a book.");
		return 0;
	}
}
