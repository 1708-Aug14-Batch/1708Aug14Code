var btn = document.getElementById("registerEmp");

btn.addEventListener("click", function() {
	var fn = document.getElementById("fname").value;
	var ln = document.getElementById("lname").value;
	var un = document.getElementById("uname").value;
	var em = document.getElementById("email").value;
	alert("the firstname entered was: " + fn);
	alert("the name is: " + $("#fname").val());
	alert("The last name entered was: " + ln);
	alert("The username entered was: " + un);
	alert("The email entered was: " + em);
});
	
	
	
