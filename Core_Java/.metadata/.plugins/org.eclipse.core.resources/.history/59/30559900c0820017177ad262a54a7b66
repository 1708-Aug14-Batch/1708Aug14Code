package com.ex.day3;

public class ThreadIntro {
	
	

}

// extends Thread
class extendClass extends Thread {
	
	public void run() {
		
		System.out.println("In extendClass");
		
		for(int i = 0; i < 10; i++)
			System.out.println(i + ": in extendClass");
	}
	
}

// implements runnable
class ImplementRunnable implements Runnable {
	
	@Override
	public void run() {
		System.out.println("In ImplementRunnable");
		
		for(int i = 0; i < 10; i++)
			System.out.println(i + ": in implementRunnable");
	}
}