package question1;

import java.util.ArrayList;

// FIXME add lots of meaningful comments

public class BubbleSortDriver {
	
	// A provided input of numbers to be sorted
	private static int[] inputArray = {1,0,5,6,3,2,3,7,9,8,4};
	
	public static void main(String[] args) {
		printArray(inputArray);
	
		int[] outputArray = bubbleSort(inputArray);
		
		printArray(outputArray);
	}
	
	private static int[] bubbleSort(int[] inputArray) {
		return inputArray;	// FIXME method stub
	}
	
	private static void printArray(int[] myArray) {
		for (int num : myArray) {
			System.out.print(num + ", ");
		}
		System.out.print("\b\b");
		System.out.println();
	}
	
}
