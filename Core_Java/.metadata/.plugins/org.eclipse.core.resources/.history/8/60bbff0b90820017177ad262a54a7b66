	}

	@Test
	public void testBoundaryCases() {
		int[] expected1 = null;
		
		// Fibonacci sequence with 0 numbers
		assertEquals(null, FibonacciNumbers.Fibonacci(0));
		
		// Fibonacci sequence with 1 number
		assertEquals(null, FibonacciNumbers.Fibonacci(0));
		
		// Fibonacci sequence with fewer than 0 numbers
		assertEquals(null, FibonacciNumbers.Fibonacci(0));
		
	}
	
	@Test
