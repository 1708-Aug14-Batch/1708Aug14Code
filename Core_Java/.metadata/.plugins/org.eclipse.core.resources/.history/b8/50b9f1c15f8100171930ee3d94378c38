package question7;

/*
 * Q7. Sort two employees based on their name, department, and
 * age using the Comparator interface.
 */

public class Driver {
	
	public static void main(String[] args) {
		
		Colletions myCollection = new Collection();
		
		Employee emp2 = new Employee("John Doe", 30, Department.TRAINING);
		Employee emp1 = new Employee("Joe Smith", 42, Department.HUMAN_RESOURCES);
		
		myCollection.add(emp1);
		myCollection.add(emp2);
		
		myCollection.sort();
	}

}
