package Q2;

public class Fibonacci {

	public static void firstNFib(int last, int next, int n) {
		if (n == 0) return;
		
		if (last == 0) {
			System.out.print(last + ", " + next + ", ");
		}
		else {
			System.out.print(next + ", ");
		}

		firstNFib(next, last + next, n-1);
	}
	
	public static void main(String[] args) {
		
		// Print first 25 numbers in the Fibonacci sequence
		firstNFib(0, 1, 25);		
	}
}
