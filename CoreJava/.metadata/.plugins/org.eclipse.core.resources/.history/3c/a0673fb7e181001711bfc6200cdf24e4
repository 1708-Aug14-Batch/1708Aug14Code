package Q4;

public class Factorial {
	
	private static long doFactorial(long fac, int n) {
		if (n == 0) return fac;
		return doFactorial(fac*n, n-1);
	}
	
	public static long factorial(int n) {
		doFactorial(1, n);
	}
	
	public static void main(String[] args) {
		
		// Print 25!
		factorial(25);
	}

}
