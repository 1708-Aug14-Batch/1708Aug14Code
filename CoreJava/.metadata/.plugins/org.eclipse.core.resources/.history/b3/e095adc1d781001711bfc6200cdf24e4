package Q2;

public class Fibonacci {

	public static void firstNFib(int last, int start, int n) {
		if (n == 0) return;
		
		if (last == 0) {
			System.out.print(last + ", " + start + ", ");
			firstNFib(start, last + start, n-1);
		}
		System.out.println("0");
		for (int i = 1; i < n; i++) {
			int temp = sum;
			sum += last;
			last = sum;
			
			System.out.println(sum);
		}
	}
	
	public static void main(String[] args) {
		
		// Print first 25 numbers in the Fibonacci sequence
		firstNFib(0, 1, 25);		
	}
}
