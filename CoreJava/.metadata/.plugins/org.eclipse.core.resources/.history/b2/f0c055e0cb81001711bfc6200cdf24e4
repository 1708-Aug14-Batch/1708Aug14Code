package Q1;

public class BubbleSort {
	
	public static void bubbleSort (int[] list) {
		int length = list.length;
		for (int i = 0; i < length--; i++) {
			if (list[i] > list[i+1]) {
				int temp = list[i];
				list[i] = list[i+1];
				list[i+1] = temp;
			}
		}
	}

	public static void main(String[] args) {
		int[] list = { 1, 0, 5, 6, 3, 2, 3, 7, 9, 8, 4 };
		
		for (int i : list) {
			System.out.print(i + ", ");
		}
		
		bubbleSort(list);
		System.out.println();
		
		for (int i : list) {
			System.out.print(i + ", ");
		}
	}
}
