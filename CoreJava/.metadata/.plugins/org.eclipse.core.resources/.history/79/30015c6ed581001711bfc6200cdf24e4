package Q2;

public class Fibonacci {

	public static void firstNFib(int n) {
		int sum = 1;
		int last = 0;
		System.out.println(0);
		for (int i = 1; i < n; i++) {
			int temp = sum;
			sum += last;
			last = sum;
			
			System.out.println(sum);
		}
	}
	
	public static void main(String[] args) {
		
		// Print first 25 numbers in the Fibonacci sequence
		firstNFib(25);		
	}
}
