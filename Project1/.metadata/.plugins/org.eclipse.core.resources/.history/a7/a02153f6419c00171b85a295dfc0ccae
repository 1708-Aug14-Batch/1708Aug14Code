package com.reimburse.pojos;

public class User {
	private int userid;	
	private String firstName;
	private String lastName;
	private String email;
	private String password;
	private int isAManager;
//	private Reimbursement[] pending;
//	private Reimbursement[] resolved;
	
	public User(){}
	
	public User(int userid, String firstName, String lastName, String email, String password,
			int isAManager) {
		super();
		this.userid = userid;		
		this.firstName = firstName;
		this.lastName = lastName;
		this.email = email;
		this.password = password;
		this.isAManager = isAManager;
	}
	
	
	
	public int getUserid() {
		return userid;
	}

	public void setUserid(int userid) {
		this.userid = userid;
	}

	public String getFirstName() {
		return firstName;
	}

	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public int getIsAManager() {
		return isAManager;
	}

	public void setIsAManager(int isAManager) {
		this.isAManager = isAManager;
	}

	@Override
	public String toString() {
		return "User [user_id=" + userid +  "password=" + password + ", firstName="
				+ firstName + ", lastName=" + lastName + ", email=" + email + ", role_id=" + isAManager + "]";
	}
	
}
