/**
 * 
 */


function fib(n) {
	if (n <= 1)
		return n;
	return fib(n-1) + fib(n-2);
}


function runFib() {
	var display = document.getElementById("fibOut");
	var n = document.getElementById("fiBin").value();
	display.innerHtml = fib(n);
}

document.getElementById("fibOut").addEventListener("click", runFib);