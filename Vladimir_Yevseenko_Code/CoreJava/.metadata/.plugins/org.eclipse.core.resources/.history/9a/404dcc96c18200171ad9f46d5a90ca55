package com.revature.threads;

public class Main {
	public static void main(String[] args) {

		Runnable r = new Runnable() {
			@Override
			public void run() {
				System.out.println("Anonymous runnable");
			}
		};
		
		Runnable lambda = () -> {
			
		};
		
		new Thread(r).start();
		System.out.println("Main");

	}
}
