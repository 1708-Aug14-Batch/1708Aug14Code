		System.out.println("Even sum: " + evenSum);
		
		System.out.println("Only non-prime numbers: " + list);
	}
	
	public static boolean isEven(int i) {
		return i % 2 == 0;
	}
	
	public static boolean isPrime(int n) {
		if (n == 1 || n == 0)
			return false;
		for (int i=2; i*i <= n; i++)
			if (n % i == 0)
				return false;
		return true;
	}
}