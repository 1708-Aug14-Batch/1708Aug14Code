public class IsEven {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter a number: ");
		try {
			
		} catch (NumberFormatException ex) {
			
		}
	}
	
	public static boolean isEven(int n) {
		return (n & 1) == 1;
	}
