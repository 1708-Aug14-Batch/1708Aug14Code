		<input type="text" id="newItem" placeholder="Anything else?" class="form-control"/>
		<button id="add">Add Item</button>
		<br/>
		<input type="text" id="toRemove" placeholder="Item to remove"/>
		<button id="remove">Remove Item</button>
		
