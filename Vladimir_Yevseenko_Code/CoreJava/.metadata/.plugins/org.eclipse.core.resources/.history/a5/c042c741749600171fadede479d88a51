$(function() {
	$.post('http://localhost:8181/ReimbursementSystem/userInfoServlet',
			{},
			function(data) {
				console.log(curUser);
				let curUser = data.user;
				if (!curUser.isManager) {
					$('#viewAllReimbursementsBtn').hide();
					$('#registerEmployeeBtn').hide();
				} else {
					$('#viewOwnReimbursementsBtn').hide();
				}
			},
			'JSON');
});

