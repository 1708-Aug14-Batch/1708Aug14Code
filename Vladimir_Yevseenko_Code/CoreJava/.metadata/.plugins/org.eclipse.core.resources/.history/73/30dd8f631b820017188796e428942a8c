package com.revature.q9;

import java.util.ArrayList;

public class Primes {
	private static ArrayList<Integer> sieve = new ArrayList<>();
	
	public static void main(String[] args) {
		initSieve();
		ArrayList<Integer> list = new ArrayList<>();
		for (int i=2; i<=100; i++)
			list.add(i);
		for (int i=2; i<=10; i++)
			for (int j=i*2; j<=100; j += i)
				list.remove((Object) i);
		
	}
	
	public static void initSieve() {
		for (int i=2; i<=100; i++)
			sieve.add(i);
		for (int i=2; i<=10; i++)
			for (int j = i * 2; j <= 100; j += i)
				sieve.remove((Object) j);
	}
}