$(function() {
	$.post('yourReimbursementsServlet',
			JSON.stringify({}),
			function(data) {
				console.log(data);
				for (let i=0; i<data.length; i++) {
					let reimb = data[i];
					console.log(reimb);
					console.log(reimb.amount);
					//$('#reimbursementList').append('<li>' + reimb.id + '</li>');
				}
			},
			'JSON');
});