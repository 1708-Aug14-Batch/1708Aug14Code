	@Test
	public void hasUpperCaseTest() {
		ConcreteStrOps c = new ConcreteStrOps();
		assertTrue(c.hasUpperCase("Vladimir"));
		assertFalse(c.hasUpperCase("catcat cat"));
		assertTrue(c.hasUpperCase("81298hdhasudsdo8q89ydD"));
	}
	
