package com.revature.pojos;

import java.sql.Date;

public class Reimbursement {
	private int id;
	private User submitter, resolver;
	private Date submission_date, resolution_date;
	private Status reimbursement_status;
	private String description;
	double amount;
	
	
	public Reimbursement() {
	}
	
	
	public Reimbursement(int id, User submitter, User resolver, Date submission_date,
			Date resolver_date, Status reimbursement_status, String description, double amount) {
		this.id = id;
		this.submitter = submitter;
		this.resolver = resolver;
		this.submission_date = submission_date;
		this.resolution_date = resolution_date;
		this.reimbursement_status = reimbursement_status;
		this.description = description;
		this.amount = amount;
	}
	
}
