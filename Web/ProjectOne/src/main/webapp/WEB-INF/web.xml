<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>ProjectOne</display-name>


	<servlet>
		<description></description>
		<display-name>Login</display-name>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.project.one.servlets.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>EmployeePortal</display-name>
		<servlet-name>EmployeePortal</servlet-name>
		<servlet-class>com.project.one.servlets.EmployeePortal</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeePortal</servlet-name>
		<url-pattern>EmployeePortal.html</url-pattern>
	</servlet-mapping>
</web-app>